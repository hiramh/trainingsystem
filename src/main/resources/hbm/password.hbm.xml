<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.synnex.model">
	<class name="PasswordInfo" table="pwinfo">
		<id column="id" name="id">
			<generator class="native" />
		</id>
		<property column="name" name="name" />
		<property column="username" name="username" />
		<property column="password" name="password" />
		<property column="url" name="url" />
		<property column="info" name="info" />
	</class>
</hibernate-mapping>